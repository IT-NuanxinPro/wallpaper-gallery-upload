{
  "name": "wallpaper-gallery-studio",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:proxy": "node server/ai-proxy.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:proxy\"",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src --fix",
    "format": "prettier --write src",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss}": [
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "vue": "^3.5.24",
    "vue3-virtual-scroller": "^0.2.3"
  },
  "devDependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "@eslint/js": "^9.39.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/test-utils": "^2.4.6",
    "@vueuse/core": "^14.1.0",
    "element-plus": "^2.13.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.6.2",
    "fast-check": "^4.5.3",
    "gsap": "^3.14.2",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "lint-staged": "^16.2.7",
    "pinia": "^3.0.4",
    "prettier": "^3.7.4",
    "sass": "^1.97.2",
    "unplugin-auto-import": "^20.3.0",
    "unplugin-vue-components": "^30.0.0",
    "vite": "^7.2.4",
    "vitest": "^4.0.16",
    "vue-router": "^4.6.4"
  }
}
